{
  "name": "astro2",
  "version": "2.0.0",
  "private": true,
  "description": "AstroZi - AI Life Engineering Platform",
  "scripts": {
    "dev": "next dev",
    "dev:simple": "next dev -p 3007",
    "dev:web3": "NEXT_PUBLIC_DEPLOYMENT_TARGET=web3 NEXT_PUBLIC_APP_MODE=web3 next dev -p 3007",
    "dev:unified": "NEXT_PUBLIC_DEPLOYMENT_TARGET=unified NEXT_PUBLIC_APP_MODE=unified next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "analyze": "ANALYZE=true next build",
    "docker:build": "docker build -t astro2-app .",
    "docker:run": "docker run -p 3000:3000 astro2-app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:mutual-aid": "jest --config tests/config/jest.config.js",
    "test:mutual-aid:watch": "jest --config tests/config/jest.config.js --watch",
    "test:mutual-aid:coverage": "jest --config tests/config/jest.config.js --coverage",
    "test:mutual-aid:ci": "jest --config tests/config/jest.config.js --ci --coverage --watchAll=false",
    "test:auth-api": "jest --config tests/config/jest.config.js --testNamePattern=\"认证\"",
    "test:integration": "jest --config tests/config/jest.config.js --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:performance": "playwright test tests/e2e/performance.spec.ts",
    "test:auth": "playwright test tests/e2e/auth.spec.ts",
    "test:api": "playwright test tests/e2e/api.spec.ts",
    "test:astrology": "playwright test tests/e2e/astrology.spec.ts",
    "test:all": "npm run test:mutual-aid && npm run test:e2e",
    "cache:clear": "node -e \"const { getCache } = require('./lib/redis'); getCache().clear().then(() => console.log('Cache cleared'))\"",
    "health:check": "curl http://localhost:3007/api/health | jq",
    "deploy:build": "pnpm build && pnpm start",
    "scripts:migrate-privy-auth-type": "tsx scripts/migrate-privy-auth-type.ts",
    "build:web3": "NEXT_PUBLIC_DEPLOYMENT_TARGET=web3 NEXT_PUBLIC_APP_MODE=web3 NEXT_PUBLIC_DEFAULT_LANGUAGE=en pnpm build",
    "build:unified": "NEXT_PUBLIC_DEPLOYMENT_TARGET=unified NEXT_PUBLIC_APP_MODE=unified NEXT_PUBLIC_DEFAULT_LANGUAGE=zh pnpm build",
    "deploy:web3": "./scripts/deployment-scripts.sh full-deploy web3",
    "deploy:unified": "./scripts/deployment-scripts.sh full-deploy unified",
    "config:check": "./scripts/deployment-scripts.sh check-env",
    "config:generate": "./scripts/deployment-scripts.sh generate-env",
    "db:migrate": "./scripts/deployment-scripts.sh migrate",
    "fix:eslint": "node scripts/fix-eslint.js",
    "fix:auto": "npm run lint -- --fix",
    "quality:check": "npm run typecheck && npm run lint",
    "quality:fix": "npm run fix:auto && npm run fix:eslint",
    "build:production": "NODE_ENV=production npm run build:web3",
    "deploy:quick": "npm run quality:fix && npm run build:production",
    "hardhat:compile": "hardhat compile",
    "hardhat:test": "hardhat test",
    "hardhat:deploy:testnet": "hardhat run contracts/deploy/deploy.js --network bsc_testnet",
    "hardhat:deploy:mainnet": "hardhat run contracts/deploy/deploy.js --network bsc_mainnet",
    "hardhat:verify": "hardhat run contracts/deploy/deploy.js --network bsc_testnet --verify",
    "hardhat:node": "hardhat node",
    "deploy:db": "supabase db push --linked",
    "migrate:db": "node scripts/run-database-migration.js",
    "setup:env": "node scripts/setup-environment.js",
    "dev:test": "NODE_ENV=test npm run dev",
    "db:generate-types": "npx supabase gen types typescript --project-id fbtumedqykpgichytumn > types/supabase.ts",
    "db:reset": "npx supabase db reset",
    "db:seed": "node scripts/seed-database.js",
    "postman:import": "echo 'Import postman/mutual-aid-auth-collection.json into Postman'",
    "docs:serve": "npx serve docs -p 3001",
    "security:scan": "npm audit --audit-level moderate",
    "pre-commit": "npm run lint:fix && npm run typecheck && npm run test:mutual-aid:ci",
    "bmad:refresh-agents-md": "node scripts/refresh-agents-md.js",
    "ingest:notion": "tsx scripts/ingest-notion.ts",
    "ingest:drive": "tsx scripts/ingest-drive.ts"
  },
